{
  "name": "flagmint-js-sdk",
  "version": "1.0.5",
  "description": "Framework-agnostic JavaScript SDK for managing feature flags in Flagmint applications.",
  "author": "Flagmint Team",
  "license": "MIT",
  "main": "dist/flagmint.cjs.js",
  "module": "dist/flagmint.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/flagmint.es.js",
      "require": "./dist/flagmint.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && vite build",
    "test": "jest --runInBand --detectOpenHandles"
  },
  "peerDependencies": {
    "crypto": "*"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.24",
    "crypto-browserify": "^3.12.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "ts-jest": "^29.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^2.0.0"
  },
  "dependencies": {
    "buffer": "^6.0.3"
  }
}
